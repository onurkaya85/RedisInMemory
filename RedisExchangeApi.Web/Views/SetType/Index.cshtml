@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form method="post" asp-action="Add" asp-controller="SetType" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit">Save</button>
</form>

@foreach(var item in Model)
{
    
    <h2>@item <a asp-controller="SetType" asp-action="DeleteItem" asp-route-name="@item">Delete</a></h2>
}

<a asp-controller="SetType" asp-action="DeleteFirstItem">Delete First Item</a>

 