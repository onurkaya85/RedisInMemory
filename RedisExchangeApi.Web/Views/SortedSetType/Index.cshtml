@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form method="post" asp-action="Add" asp-controller="SortedSetType" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
     <input type="number" name="score" />
    <button type="submit">Save</button>
</form>

@foreach(var item in Model)
{
    
    <h2>@item <a asp-controller="SortedSetType" asp-action="DeleteItem" asp-route-name="@item">Delete</a></h2>
}

 